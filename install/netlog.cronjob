#Netlog components
* * * * *       root    /usr/bin/php /usr/share/netlog/core/logscavenger.php 1>/dev/null 2>&1
* * * * *       root    /usr/bin/php /usr/share/netlog/core/lograte.php 1>/dev/null 2>&1
0 1 * * *       root    /usr/bin/php /usr/share/netlog/core/logarchiver.php 1>/dev/null 2>&1
0 1 * * *       root    /usr/bin/php /usr/share/netlog/core/prunelograte.php 1>/dev/null 2>&1
*/5 * * * *     root    [ ! "$(systemctl is-active logparser)" == "active" ] && systemctl restart logparser
